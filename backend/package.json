{
  "name": "@whiterabbitjs/dashboard-backend",
  "version": "0.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start-local": "cross-env NODE_ENV=dev ts-node-dev src/server.ts",
    "start-prod": "npm run build && node dist/server.js",
    "start-db": "npx local-dynamo --database_dir=./db --port=4567",
    "build": "ncp src/schema dist/schema && tsc",
    "lint": "npx eslint src/",
    "test": "npx jest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws/dynamodb-data-mapper": "^0.7.3",
    "@aws/dynamodb-data-mapper-annotations": "^0.7.3",
    "apollo-datasource-rest": "^0.9.0",
    "apollo-server": "^2.13.1",
    "apollo-server-express": "^2.13.1",
    "aws-sdk": "^2.683.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "graphql": "^15.0.0",
    "graphql-import-node": "0.0.4"
  },
  "devDependencies": {
    "@types/compression": "^1.7.0",
    "@types/express": "^4.17.6",
    "@types/jest": "^25.2.1",
    "@types/node": "^14.0.1",
    "@types/supertest": "^2.0.9",
    "@typescript-eslint/eslint-plugin": "2.33.0",
    "@typescript-eslint/parser": "2.33.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.0.0",
    "jest": "^26.0.1",
    "jest-transform-graphql": "^2.1.0",
    "local-dynamo": "^0.6.0",
    "ncp": "^2.0.0",
    "supertest": "^4.0.2",
    "ts-jest": "^25.5.1",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.9.2"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "transform": {
      "\\.(gql|graphql)$": "jest-transform-graphql"
    }
  }
}
