{
  "compilerOptions": {
    "declaration": true,
    "removeComments": true,
    "noLib": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "esModuleInterop": true,
    "target": "esnext",
    "sourceMap": true,
    "strict": true,
    "allowSyntheticDefaultImports": true,
